            "post": {
                "description": "save passport in database",
                "consumes": [
                    "multipart/form-data",
                    "text/xml"
                ],
                "produces": [
                    "text/xml"
                ],
                "tags": [
                    "passports"
                ],
                "summary": "SavePassport",
